<script type="text/javascript">
    
    function aaTrackDownload(downloadButton) {
        if (downloadButton && downloadButton.parentElement) {
            var inputElement = downloadButton.parentElement.querySelector('span.download-list-input');

            if (inputElement) {
                var inputJson = JSON.parse(inputElement.dataset.input);
                let downloadFileType = inputJson.DownloadFileType;

                // 選択チェックボックスが選択状態のメディアのファイル名(拡張子なし)を取得する
                var container = document.getElementById(inputJson.DownloadFileContainerID);
                var checkboxesSelected = container.querySelectorAll('input[type="checkbox"]:checked');
                var checkboxesSelectedArray = Array.prototype.slice.call(checkboxesSelected, 0); 

                const selectedFilesArray = [];
                for (let i=0; i<checkboxesSelectedArray.length; i++)
                {
                    if (checkboxesSelectedArray[i].dataset.filename && (selectedFilesArray.indexOf(checkboxesSelectedArray[i].dataset.filename) < 0)) {
                        selectedFilesArray.push(checkboxesSelectedArray[i].dataset.filename);
                    };
                }
                
                // １つ以上のメディアが選択されていれば、Adobe Analytics計測用関数を呼び出す
                if (selectedFilesArray.length >= 1) {
                    s.mrt_downloadLinkTrack({fileType:downloadFileType,fileName:selectedFilesArray.join(),aObj:this});
                }
            }
        }
        return true;
    }
    
</script>


