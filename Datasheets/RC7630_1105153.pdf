<script type="text/javascript">
    function htmlEncode(value) {
        return $('<div/>').text(value).html().replace(/&/g, '%26');
    }
    function htmlDecode(value) {
        return $('<div/>').html(value).text();
    }
    jQuery(function () { 
        var $modal = $('#pickProductOfInterests');
        var keyboard = false; // Prevent to close by ESC
        var backdrop = 'static'; // Prevent to close on click outside the modal
        var show = false;

        if (typeof $modal.data('bs.modal') === 'undefined') { // Modal did not open yet
            $modal.modal({
                keyboard: keyboard,
                backdrop: backdrop,
                show: show
            });
        } else { // Modal has already been opened
            $modal.data('bs.modal').options.keyboard = keyboard;
            $modal.data('bs.modal').options.backdrop = backdrop;
            $modal.data('bs.modal').options.show = show;

            if (keyboard === false) {
                $modal.off('keydown.dismiss.bs.modal'); // Disable ESC
            } else { // 
                $modal.data('bs.modal').escape(); // Resets ESC
            }
        }

        $modal.on('show.bs.modal', function (e) {
            $("#headerWrapper").css('z-index', 0);
            $("#homeProductSelectorCarouselWrapper").css('z-index', 0);
        });

        $modal.on('hide.bs.modal', function (e) {
            setTimeout(function () {
                $("#headerWrapper").css('z-index', '');
                $("#homeProductSelectorCarouselWrapper").css('z-index', '');
            }, 500);
        });

        $("#ctl14_dwnProductOfInterest").change(function () {
            if ($("#ctl14_dwnProductOfInterest").val()) {
                $("#productInterestErrorMessage").hide();
            }
            else {
                $("#productInterestErrorMessage").show();
            }
        });

        $("#btnSubmitProductOfIterests").on("click", function (event) {
            var userInfoEndpoint = '';
        var pageSSOSendToEloqua = 'https://sierrawireless.com/sso/sendToEloqua';
        jQuery.getResults(userInfoEndpoint, function (result) {
            //success
            if (result) {
                if (result.name) {
                    $("#ctl14_hpCurrentUserProfile").text(result.name);
                } else if (result.email) {
                    $("#ctl14_hpCurrentUserProfile").text(result.email);
                }

                var payload = result;

                //server call

                var url = pageSSOSendToEloqua.toLowerCase();
                var productOfInterest = $("#ctl14_dwnProductOfInterest").val();

                payload.productInterest = productOfInterest;

                if (!payload.productInterest) {
                    $("#productInterestErrorMessage").show();
                    return;
                } else {
                    $("#productInterestErrorMessage").hide();
                }

                jQuery.postAndGetResults(url, payload, function (postToEloquaResult) {
                    console.log(postToEloquaResult)
                    if (postToEloquaResult.success) {
                        $('#pickProductOfInterests').modal('hide');
                    }
                    }, function () {
                        //complete
                    });
            }
        },
            function (a, b, c) {
                //error
                console.log(c);
                if (c === "Unauthorized") {
                    window.location.href = "https://sierrawireless.com/sso/login" + '?redirectUrl=' + window.location.protocol + "//" + window.location.host + window.location.pathname;;
                }
            },
            function () {
                //complete
            });
        });
    });
    
   
</script>

