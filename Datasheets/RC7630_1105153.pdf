<script>
function getipdata(apikey, callback) {
    var ipdataUrl = "https://api.ipdata.co/";
    var params = apikey;
    var xhttp = new XMLHttpRequest(); 
    
    xhttp.addEventListener('load', callback);
    xhttp.addEventListener('error', function() { console.log("Request to "+ipdataUrl+params+" failed"); });
    
    xhttp.open("GET", ipdataUrl + params, true);
    xhttp.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
    xhttp.send();
}

function getgeojs(callback) {
    var geoJsUrl = "https://get.geojs.io/v1/ip/geo.json";
    var xhttp = new XMLHttpRequest(); 
    
    xhttp.addEventListener('load', callback);
    xhttp.addEventListener('error', function() { console.log("Request to "+ geoJsUrl +" failed"); });
    
    xhttp.open("GET", geoJsUrl, true);
    xhttp.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
    xhttp.send();
}

