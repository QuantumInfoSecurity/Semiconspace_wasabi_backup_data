            function IsValidCtaForm(email) {
                if (!email || email === "") {
                    if (jQuery(".window-submit-error-container").hasClass("hide")) {
                        jQuery(".window-submit-error-container").removeClass("hide");
                    }

                    if (jQuery(".email-required").hasClass("hide")) {
                        jQuery(".email-required").removeClass("hide");
                    }

                    if (!jQuery(".email-notvalid").hasClass("hide")) {
                        jQuery(".email-notvalid").addClass("hide");
                    }

                    return false;
                }

                var emailRegex = /^[A-Z0-9_'%=+!`#~$*?^{}&|-]+([\.][A-Z0-9_'%=+!`#~$*?^{}&|-]+)*@[A-Z0-9-]+(\.[A-Z0-9-]+)+$/i;
                if (!emailRegex.test(email)) {
                    if (jQuery(".window-submit-error-container").hasClass("hide")) {
                        jQuery(".window-submit-error-container").removeClass("hide");
                    }

                    if (!jQuery(".email-required").hasClass("hide")) {
                        jQuery(".email-required").addClass("hide");
                    }

                    if (jQuery(".email-notvalid").hasClass("hide")) {
                        jQuery(".email-notvalid").removeClass("hide");
                    }

                    return false;
                }

                if (!jQuery(".window-submit-error-container").hasClass("hide")) {
                    jQuery(".window-submit-error-container").addClass("hide");
                }

                if (!jQuery(".email-required").hasClass("hide")) {
                    jQuery(".email-required").addClass("hide");
                }

                if (!jQuery(".email-notvalid").hasClass("hide")) {
                    jQuery(".email-notvalid").addClass("hide");
                }

                return true;
            }

