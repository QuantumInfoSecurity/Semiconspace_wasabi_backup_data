		<script>
document.addEventListener("DOMContentLoaded", function() {
  let partNumber = "";

  //  Page Type A: Static part number
  const typeA = document.getElementById("config-part-number");
  if (typeA) {
    partNumber = typeA.textContent.trim();
    console.log("Page Type A detected. Part Number:", partNumber);
  }

  //  Page Type B: Get part number from nearby <tr class="param-detail-trigger">
  function getScopedPartNumber(trigger) {
    let current = trigger.closest("tr");
    while (current && !current.classList.contains("param-detail-trigger")) {
      current = current.previousElementSibling;
    }

    if (current && current.hasAttribute("data-part-number")) {
      const foundPart = current.getAttribute("data-part-number").trim();
      console.log("Scoped part number found:", foundPart);
      return foundPart;
    }

    console.log("No scoped part number found.");
    return "";
  }

  //  Fill the correct Gravity Forms field (ID 7 or 9)
  function fillFormField(value) {
    const waitForField = setInterval(function() {
      const input7 = document.querySelector("input[name='input_7']");
      const input9 = document.querySelector("input[name='input_9']");

      if (input7 && input7.offsetParent !== null) {
        input7.value = value;
        input7.readOnly = true;
        console.log("Filled input_7 with:", value);
        clearInterval(waitForField);
      } else if (input9 && input9.offsetParent !== null) {
        input9.value = value;
        input9.readOnly = true;
        console.log("Filled input_9 with:", value);
        clearInterval(waitForField);
      }
    }, 100); // Fast checking
  }

  //  Listen for any CAD Models button click (works for both Page Types)
  document.body.addEventListener("click", function(e) {
    const trigger = e.target.closest("button.btn-cad-link");
    if (trigger) {
      console.log("CAD Models button clicked.");

      setTimeout(function() {
        let scopedPartNumber = getScopedPartNumber(trigger);

        //  If Page Type A, always use the static part number
        if (partNumber) {
          scopedPartNumber = partNumber;
        }

        if (scopedPartNumber) {
          fillFormField(scopedPartNumber);
        } else {
          console.log("No part number found to fill.");
        }
      }, 600); // Delay to allow form popup to render
    }
  });
});
