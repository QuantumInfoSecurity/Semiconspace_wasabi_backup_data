<script>
  // Constants
  const getOptionsURL = "https://www.e-switch.com/wp-admin/admin-ajax.php?action=eswitch_get_available_options";
  const VALID_SELECT_FIELDS = ["switch_category","series","current_rating","voltage_rating","circuit","switch_function","ingress_protection","illumination_lens_style","mounting_type","termination_type","actuator_type_style","actuator_height","operating_force","illumination_lens_color","panel_cutout_dimensions"];
  const PRIMARY_FIELD = 'switch_category';
  
  // DOM Elements
  const form = document.getElementById('findproduct-form');
  const selectDropdowns = document.querySelectorAll('.parametric_select');
  const submitButton = document.getElementById('product-search-submit');
  
  // State Management
  let isInitializing = false;

  /** UTILITY FUNCTIONS */
  
  /**
   * Parse query string parameters from current URL
   * @returns {Object} Object containing query parameters
   */
  function parseQueryString() {
    const urlParams = new URLSearchParams(window.location.search);
    const queryParams = {};
    
    VALID_SELECT_FIELDS.forEach(field => {
      const value = urlParams.get(field);
      if (value && value.trim() !== '') {
        queryParams[field] = decodeURIComponent(value.trim());
      }
    });
    
    return queryParams;
  }

  /**
   * Populate a select dropdown with options
   * @param {HTMLSelectElement} selectElement 
   * @param {Array} options 
   * @param {string} selectedValue 
   */
  function populateSelectOptions(selectElement, options, selectedValue = null) {
    // Clear existing options except the first (placeholder)
    while (selectElement.options.length > 1) {
      selectElement.remove(1);
    }
    
    // Filter out invalid options
    const validOptions = (options || []).filter(option => 
      option !== null && option !== undefined && option !== '' && option !== 'null'
    );
    
    if (validOptions.length === 0) {
      selectElement.disabled = true;
      return false;
    }
    
    selectElement.disabled = false;
    validOptions.forEach(option => {
      const opt = document.createElement('option');
      opt.value = option;
      opt.textContent = option;
      selectElement.appendChild(opt);
    });
    
    // Set selected value if provided and exists in options
    if (selectedValue && validOptions.includes(selectedValue)) {
      selectElement.value = selectedValue;
      return true;
    }
    
    return false;
  }

  /**
   * Initialize form with query string values and populate dropdowns
   */
  async function init() {
    try {
      isInitializing = true;
      const queryParams = parseQueryString();
      
      // Always populate the primary field (switch_category) first
      const typeSelect = document.getElementById(PRIMARY_FIELD);
      const typeOptions = await getAvailableOptions(PRIMARY_FIELD);
      const typeSelected = populateSelectOptions(typeSelect, typeOptions, queryParams[PRIMARY_FIELD]);
      
      // If switch_category has a value (from query or manual selection), enable other fields
      if (typeSelected || typeSelect.value !== '') {
        await initializeSecondaryFields(queryParams);
        submitButton.disabled = false;
      }
      
    } catch (error) {
      console.error('Error during form initialization:', error);
    } finally {
      isInitializing = false;
    }
  }

  /**
   * Initialize secondary fields based on selected primary field and query parameters
   * @param {Object} queryParams 
   */
  async function initializeSecondaryFields(queryParams) {
    // Enable all secondary dropdowns
    selectDropdowns.forEach(select => {
      if (select.id !== PRIMARY_FIELD) {
        select.disabled = false;
      }
    });

    // Build complete selectedOptions that includes current form state AND query parameters
    // This ensures filtering includes all selections, not just the primary field
    const currentFormOptions = getFormSelectedOptions();
    const selectedOptions = { ...currentFormOptions, ...queryParams };
    
    // Collect all secondary field names for batch processing
    const secondaryFields = [];
    selectDropdowns.forEach(select => {
      if (select.id !== PRIMARY_FIELD) {
        secondaryFields.push(select.name);
      }
    });
    
    try {
      // Batch request for all secondary fields using complete filter set
      const allFieldOptions = await getAvailableOptions(secondaryFields, selectedOptions);
      
      // Process each secondary field with its options
      selectDropdowns.forEach(select => {
        if (select.id === PRIMARY_FIELD) return;
        
        const fieldOptions = allFieldOptions[select.name] || [];
        populateSelectOptions(select, fieldOptions, queryParams[select.name]);
      });
      
    } catch (error) {
      console.error('Error during batch field initialization:', error);
      // Fallback to individual field processing if batch fails
      await initializeSecondaryFieldsFallback(queryParams);
    }
  }

  /**
   * Fallback method for initializing secondary fields individually
   * @param {Object} queryParams 
   */
  async function initializeSecondaryFieldsFallback(queryParams) {
    // Build complete selectedOptions that includes current form state AND query parameters
    const currentFormOptions = getFormSelectedOptions();
    const selectedOptions = { ...currentFormOptions, ...queryParams };
    
    // Populate each secondary field individually
    for (const select of selectDropdowns) {
      if (select.id === PRIMARY_FIELD) continue;
      
      try {
        const fieldOptions = await getAvailableOptions(select.name, selectedOptions);
        populateSelectOptions(select, fieldOptions, queryParams[select.name]);
        
      } catch (error) {
        console.error(`Error populating field ${select.name}:`, error);
        select.disabled = true;
      }
    }
  }

  // Function to fetch available options for a given field or fields
  async function getAvailableOptions(fields, selectedOptions = {}) {
    try {
      const url = new URL(getOptionsURL);
      
      // Support single field as string or multiple as array
      if (Array.isArray(fields)) {
        url.searchParams.append('desired_field', fields.join(','));
      } else {
        url.searchParams.append('desired_field', fields);
      }

      // Add selected options as query params
      if (selectedOptions && typeof selectedOptions === 'object') {
        Object.entries(selectedOptions).forEach(([key, value]) => {
          if (value !== undefined && value !== null && value !== '') {
            url.searchParams.append(key, value);
          }
        });
      }
      
      const response = await fetch(url);
      const data = await response.json();
      
      if (data.success) {
        // Handle both single field response (array) and multi-field response (object)
        const records = data.data.records || [];
        
        if (Array.isArray(fields)) {
          // Multi-field request - return the structured object
          return records;
        } else {
          // Single field request - return array for backward compatibility
          return records;
        }
      } else {
        console.error('Error fetching options:', data.data?.message || 'Unknown error');
        return Array.isArray(fields) ? {} : [];
      }
    } catch (error) {
      console.error('Network error fetching options:', error);
      return Array.isArray(fields) ? {} : [];
    }
  }

  // Utility function to get all selected options from the form as an object
  function getFormSelectedOptions() {
    const formData = new FormData(form);
    const selectedOptions = {};
    
    for (const [name, value] of formData.entries()) {
      if (value !== undefined && value !== null && value !== '') {
        selectedOptions[name] = value;
      }
    }
    
    return selectedOptions;
  }

  // Legacy function for backward compatibility - returns array format
  function getSelectedOptions() {
    const selectedOptions = getFormSelectedOptions();
    return Object.entries(selectedOptions).map(([name, value]) => ({ name, value }));
  }

  // Utility Function - Reset Form to Default State
  function resetForm() {
    form.reset();
    selectDropdowns.forEach(function(select) {
      select.disabled = true;
      if (select.id === PRIMARY_FIELD) {
        select.disabled = false;
      }
    });

    // disable search submit btn
    submitButton.disabled = true;
    
    // Clear URL parameters to prevent re-initialization with old values
    if (window.history.replaceState) {
      const url = new URL(window.location);
      VALID_SELECT_FIELDS.forEach(field => {
        url.searchParams.delete(field);
      });
      window.history.replaceState({}, document.title, url.pathname);
    }
  }


  /** EVENT LISTENERS */
  // Startup handler to populate select options and handle query string autofill
  document.addEventListener("DOMContentLoaded", init);

  // Reset form handler
  document.getElementById('reset-filters').addEventListener('click', resetForm);

  // Collect form values and redirect to parametric search page with query strings
  submitButton.addEventListener('click', function(event) {
    event.preventDefault();
    const formData = new FormData(form);
    const queryParams = new URLSearchParams();

    for (const [key, value] of formData.entries()) {
      if (value) { // Only include non-empty values
        queryParams.append(key, value);
      }
    }

    window.location.href = '/parametric-search?' + queryParams.toString();
  });

  // Toggle visibility of additional attributes
  document.getElementById('product-search-more').addEventListener('click', function() {
    document.querySelectorAll('.show-more').forEach(el => el.classList.toggle('hidden'));
    this.classList.toggle('active');
    this.setAttribute('aria-expanded', this.getAttribute('aria-expanded') === 'true' ? 'false' : 'true');
    this.textContent = this.classList.contains('active') ? 'Fewer Attributes' : 'More Attributes';
  });

  // Primary dropdown change handler
  selectDropdowns.forEach(function(select) {
    select.addEventListener('change', function(event) {
      // Skip processing during initialization to avoid conflicts
      if (isInitializing) {
        return;
      }

      handleSelectChange(select, event);
    });
  });

  /**
   * Handle select dropdown change events
   * @param {HTMLSelectElement} select 
   * @param {Event} event 
   */
  function handleSelectChange(select, event) {
    // Special handler for switch_category
    if (select.id === PRIMARY_FIELD) {
      handlePrimaryFieldChange(event.target.value);
      return;
    }
    
    // Handle secondary field changes
    updateDependentFields();
  }

  /**
   * Handle primary field (switch_category) change
   * @param {string} selectedValue 
   */
  function handlePrimaryFieldChange(selectedValue) {
    if (selectedValue !== '') {
      // Enable all secondary dropdowns and reset their values
      selectDropdowns.forEach(function(select) {
        select.disabled = false;
        if (select.id !== PRIMARY_FIELD) {
          select.value = ''; // Reset other selects
        }
      });

      // Enable search button
      submitButton.disabled = false;
      
      // Update dependent fields
      updateDependentFields();
    } else {
      // Dropdown was reset
      resetForm();
    }
  }

  /**
   * Update dependent fields based on current form state
   */
  async function updateDependentFields() {
    const selectedOptions = getFormSelectedOptions();
    
    // Collect all empty dropdowns that need updating
    const fieldsToUpdate = [];
    selectDropdowns.forEach(dropdown => {
      // Treat null, undefined, or empty string as empty
      if (dropdown.value === null || dropdown.value === undefined || dropdown.value === '' || dropdown.value === 'null') {
        fieldsToUpdate.push(dropdown.name);
      }
    });
    
    if (fieldsToUpdate.length === 0) {
      return; // No fields need updating
    }
    
    try {
      // Batch request for all fields that need updating
      const allFieldOptions = await getAvailableOptions(fieldsToUpdate, selectedOptions);
      
      // Update each field with its options
      selectDropdowns.forEach(dropdown => {
        if (fieldsToUpdate.includes(dropdown.name)) {
          const fieldOptions = allFieldOptions[dropdown.name] || [];
          populateSelectOptions(dropdown, fieldOptions);
          
          // Disable field if no options available
          if (fieldOptions.length === 0) {
            dropdown.disabled = true;
          }
        }
      });
      
    } catch (error) {
      console.error('Error during batch field update:', error);
      // Fallback to individual field processing if batch fails
      await updateDependentFieldsFallback(fieldsToUpdate, selectedOptions);
    }
  }

  /**
   * Fallback method for updating dependent fields individually
   * @param {Array} fieldsToUpdate 
   * @param {Object} selectedOptions 
   */
  async function updateDependentFieldsFallback(fieldsToUpdate, selectedOptions) {
    for (const dropdown of selectDropdowns) {
      if (fieldsToUpdate.includes(dropdown.name)) {
        try {
          const options = await getAvailableOptions(dropdown.name, selectedOptions);
          populateSelectOptions(dropdown, options);
        } catch (error) {
          console.error(`Error updating field ${dropdown.name}:`, error);
          dropdown.disabled = true;
        }
      }
    }
  }
</script>            
